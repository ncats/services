    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@loopback/build": "^2.1.0",
    "@loopback/http-caching-proxy": "^1.2.2",
    "@loopback/repository": "^1.8.0",
    "@loopback/testlab": "^1.9.5",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
